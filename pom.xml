        <repository>
            <id>nexus</id>
            <name>Team Nexus Repository</name>
            <url>http://nexus.mucang.cn:8081/nexus/content/groups/public</url>
            <snapshots>
                <checksumPolicy>warn</checksumPolicy>
                <enabled>true</enabled>
                <updatePolicy>interval:-1</updatePolicy>
